{
    "name": "ntimyeboah/cloudtrail-management-events-to-slack",
    "description": "AWS Lambda function that sends Cloudtrail management events to Slack",
    "keywords": [
        "aws",
        "lambda",
        "cloudtrail",
        "managment",
        "events",
        "slack"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ntim Yeboah",
            "email": "ntimobedyeboah@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "bref/bref": "^2.3",
        "guzzlehttp/guzzle": "^7.0",
        "bref/secrets-loader": "^1.1",
        "ntimyeboah/php-slack": "^0.0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "NtimYeboah\\Cloudtrail\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NtimYeboah\\Cloudtrail\\Tests\\": "tests/"
        }
    }
